<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.mahe.smartwatch30.time">

    <fragment
        android:id="@+id/fragment5"
        android:name="com.example.mahe.smartwatch30.timepic"
        android:layout_width="150dp"
        android:layout_height="230dp"
        tools:layout="@layout/timepic"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="98dp"
        android:layout_marginStart="98dp" />

    <fragment
        android:id="@+id/fragment9"
        android:name="com.example.mahe.smartwatch30.timetext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout="@layout/timetext"
        android:layout_above="@+id/button11"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/button11"
        android:layout_width="wrap_content"
        android:layout_height="75dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentStart="true"
        android:onClick="back"
        android:text="BACK" />

</RelativeLayout>
