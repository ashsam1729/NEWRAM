<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.mahe.smartwatch30.newprofile">

    <fragment
        android:id="@+id/fragment2"
        android:name="com.example.mahe.smartwatch30.picture"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout="@layout/picture"
        tools:layout_height="75dp"
        tools:layout_width="75dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/button9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/fragment4"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:text="BACK"
        android:onClick="back"/>

    <fragment
        android:id="@+id/fragment7"
        android:name="com.example.mahe.smartwatch30.bottomfrag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout="@layout/bottomfrag"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
